{
    "tasks": [
        {
            "type": "shell",
            "label": "C/C++: gcc build active file",
            "command": "/usr/bin/gcc",
            "args": [
                "-g",
                "-I${workspaceFolder}/include",
                "${workspaceFolder}/kernel/main.c",
                "${workspaceFolder}/kernel/init.c",
                "${workspaceFolder}/lib/kernel/print.S",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}.out"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
     
    ],
    "version": "2.0.0"
}